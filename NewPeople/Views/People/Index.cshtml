@model List<People>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<a href="/People/AddForm" class="btn btn-dark">Add New</a>
<table>
    <tbody>
        @foreach (People person in Model)
        {
        <tr>
            <td>@person.firstname</td>
            <td>@person.lastname</td>
            <td>@person.phone</td>
            <td>@person.email</td>
            <td><a href="/people/editform/@person.id" class="btn btn-primary">Edit</a>
            <a href="/people/questiondelete/@person.id" class="btn btn-danger">Delete</a></td>
        </tr>

        }
    </tbody>
</table>
